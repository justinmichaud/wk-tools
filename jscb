#!/bin/bash

cd $BUILDDIR

echo $BUILDDIR

if [ "$(uname)" == "Darwin" ]; then

if [[ "$CONFIG" == "Release" ]]
then
    build-jsc --release --export-compile-commands
else
    build-jsc --debug --export-compile-commands
fi

else

if [[ "$CONFIG" == "Release" ]]
then
    cd WebKitBuild/JSCOnly/Release/ && time ninja -k 0 -j`echo $jobs` jsc
else
    cd WebKitBuild/JSCOnly/Debug/ && time ninja -k 0 -j`echo $jobs` jsc
fi

fi
