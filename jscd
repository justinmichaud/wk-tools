export VM=$BUILDDIR/WebKitBuild/JSCOnly/$CONFIG/
export LD_LIBRARY_PATH=$VM
export JSC_validateOptions=1
lldb -s ~/Development/wk-tools/lldb-run-file $VM/bin/jsc -- --validateOptions=1 --useGDBJITInfo=0 "$@"
